{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(scripts|assets|abis)/(.*)",
      "dest": "/frontend/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ]
}